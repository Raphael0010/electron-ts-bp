{
    "appId": "me.raphael0011.electron-ts-bp",
    "productName": "electron-ts-bp",
    "mac": {
      "category": "public.app-category.games"
    },
    "dmg": {
      "contents": [{
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": ["AppImage"]
    },
    "win": {
      "target": [{
        "target": "nsis",
        "arch": [
          "x64",
          "ia32"
        ]
      }],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }